<?xml version="1.0" encoding="UTF-8" ?>
<templates id="template" xml:space="preserve">
    <t t-name="pos_discount_price_tag.Orderline" t-inherit="point_of_sale.Orderline" t-inherit-mode="extension">
         <xpath expr="//span[hasclass('text-wrap')]" position="after">
            <t t-if="this.props.line.discount_tag_id">
                <td>
                <span>Discount:</span>
                    <span> <t t-out="this.props.line.discount_tag_id[1]"/></span>
                </td>
                </t>
        </xpath>
        <xpath expr="//div[hasclass('product-price')]" position="after">
            <tr>
            <div>
                <span>
                    <p>rtyui</p>
                </span>
            </div>
            </tr>
        </xpath>
    </t>
</templates>

