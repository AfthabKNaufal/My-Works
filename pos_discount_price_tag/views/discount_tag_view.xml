<?xml version="1.0" encoding="UTF-8" ?>
<odoo>
    <record id="discount_tag_view_action" model="ir.actions.act_window">
        <field name="name">Discount Tag</field>
        <field name="res_model">discount.tags</field>
        <field name="view_mode">tree</field>
    </record>

    <record id="discount_tag_view_tree" model="ir.ui.view">
        <field name="name">discount tags tree</field>
        <field name="model">discount.tags</field>
        <field name="arch" type="xml">
            <tree editable="bottom">
                <field name="name"/>
            </tree>
        </field>
    </record>
</odoo>