<?xml version="1.0" encoding="UTF-8" ?>
<odoo>
    <template id="new_machine_snippet" name="New Machines">
        <section class="machine_snippet">
            <div class="ref-arrival-content">
                <h1 class="snippet_heading">New Machines</h1>
                 <div id="new_machines_carousel">
                 </div>
            </div>

        </section>
    </template>

    <template id="external_snippet" inherit_id="website.snippets">
        <xpath expr="//div[@id='snippet_feature']//t[@t-snippet][last()]" position="after">
           <t t-snippet="machine_management.new_machine_snippet"/>
   </xpath>
    </template>
</odoo>