<?xml version="1.0" encoding="utf-8"?>
<odoo>
	<!--	Action to show the Dashboard-->
	<record id="file_explorer_action" model="ir.actions.client">
    	<field name="name">File Explorer</field>
    	<field name="tag">file_explorer</field>
	</record>
	<!--	Menu Item to show the dashboard menu in the module-->
	<menuitem name="FileExplorer" id="file_explorer_menu"
          	sequence="1" action="file_explorer_action"/>
</odoo>